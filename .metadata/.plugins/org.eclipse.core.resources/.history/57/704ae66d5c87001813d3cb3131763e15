
class C
{
	void f1()
	{
		System.out.println("this is class A");
	}
	
}
class B extends C
{
	void f2()
	{
		System.out.println("this is class B");
	}
}
public class singlei {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		C obj=new B();
		obj.f1();
		obj.f2();
	}

}
