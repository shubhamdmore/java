
public class Min {

	/**
	 * @param args
	 */
	static int min(int arr[])
	{
		int min = arr[0];
		for(int i=0;i<arr.length;i++)
		{
			if(min<arr[i])
			{
				min=arr[i];
			}
		}
		return min;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
    int a[]={20,50,60,30,40,10};
    int m = min(a);
    System.out.println("min value = "+m);
	}

}
